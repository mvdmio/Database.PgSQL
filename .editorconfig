resharper_max_formal_parameters_on_line = 6
resharper_max_invocation_arguments_on_line = 6
resharper_max_primary_constructor_parameters_on_line = 10000
resharper_wrap_array_initializer_style = chop_if_long
resharper_wrap_before_primary_constructor_declaration_lpar = true
resharper_wrap_before_primary_constructor_declaration_rpar = true
resharper_wrap_chained_binary_expressions = chop_if_long
resharper_wrap_chained_binary_patterns = chop_if_long
resharper_csharp_max_line_length = 300
resharper_csharp_wrap_arguments_style = chop_if_long
resharper_max_invocation_arguments_on_line = 5
resharper_csharp_keep_blank_lines_in_code = 1
resharper_csharp_wrap_lines = false
resharper_keep_existing_linebreaks = false
resharper_arguments_skip_single = true
resharper_csharp_allow_far_alignment = true
resharper_csharp_wrap_after_invocation_lpar = true
resharper_max_initializer_elements_on_line = 1

csharp_preserve_single_line_blocks = true
csharp_indent_raw_literal_string = indent

